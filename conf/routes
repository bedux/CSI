# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index

GET     /addRepo                           controllers.Registration.repoRegistrationTemplate

POST     /addRepo                           controllers.Registration.repoRegistrationSubmit

POST     /addRepoId/:id                           controllers.Application.getPrecomputedUrl(id:Long)

GET     /getAllRepository                           controllers.Application.getRepositories


GET     /testing                           controllers.Registration.getFiles

GET     /renderRepo/:id                   controllers.Application.renderRepo(id:String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)



POST /getData              controllers.Application.indexGet